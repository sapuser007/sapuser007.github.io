<mvc:View controllerName="LMES.CovidTracker2021.controller.Worklist" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc"
	xmlns:semantic="sap.f.semantic" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Page title="Please Select Your State" class="sapUiSizeCompact"> 	
		<content>
			<f:SimpleForm editable="true">
				<f:content>
					<subHeader>
						<OverflowToolbar>
							<SearchField id="searchState" liveChange="onSearch" placeholder="Search State" width="100%"/>
						</OverflowToolbar>
					</subHeader>						
					<List id="listIdStateList" items="{Data>/Table}" mode="SingleSelectMaster" selectionChange="onListStateSelect">
						<ObjectListItem title="{Data>state_name}"></ObjectListItem>
					</List>
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>
