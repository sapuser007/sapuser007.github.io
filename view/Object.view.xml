<mvc:View controllerName="LMES.CovidTracker2021.controller.Object" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.f.semantic"
	xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout">
	<Page title="Please Select Your District" showNavButton="true" navButtonPress="onNavBack" class="sapUiSizeCompact">

		<content>
			<f:SimpleForm editable="true">
				<f:content>
	<subHeader>
			<OverflowToolbar>
				<SearchField id="searchDistrict" liveChange="onSearchDistrict" placeholder="Search District"/>
			</OverflowToolbar>
		</subHeader>					
					<VBox>
						<VBox>
							<DatePicker id="DP1" placeholder="Select Date" change="handleChange" class="sapUiSmallMarginBottom" width="36%" valueFormat="dd.MM.YYYY"
								displayFormat="dd.MM.YYYY"/>
						</VBox>
						<VBox>
							<List id="listIdDistrictList" items="{DataDistrict>/Table}" mode="SingleSelectMaster" selectionChange="onListDistrictSelect">
								<ObjectListItem title="{DataDistrict>district_name}"></ObjectListItem>
							</List>
						</VBox>
					</VBox>
				</f:content>
			</f:SimpleForm>
		</content>
	</Page>
</mvc:View>
